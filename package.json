{
  "name": "@mrpotatoes/professor-frisby-fp",
  "version": "1.0.0",
  "description": "Composable Functional JS course from egghead",
  "scripts": {
    "compile": "tsc",
    "todo": "leasot -x --reporter markdown 'lessons/**/*.js' > TODO.md",
